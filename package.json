    "sass-embedded": "^1.83.4",
    "sass-loader": "16.0.4",
    "sass-true": "6.0.1",
    "sinon": "12.0.1",
    "snyk": "1.1295.3",
    "style-loader": "3.3.3",
    "stylelint": "16.12.0",
    "svgo": "^3.3.2",
    "twigjs-loader": "1.0.3",
    "typescript": "5.7.3",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0",
    "vite": "^6.2.2",
    "vite-plugin-svg-sprite": "^0.6.2",
    "webpack": "5.98.0",
    "webpack-cli": "5.1.4"
  }

}
